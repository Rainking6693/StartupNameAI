{
  "name": "startupnamer-client",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://startupnamer.org",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "2.3.0",
    "@stripe/stripe-js": "2.1.0",
    "axios": "1.6.2",
    "dotenv-expand": "^12.0.3",
    "framer-motion": "4.1.17",
    "js-cookie": "3.0.5",
    "lucide-react": "0.105.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "3.1.4",
    "react-helmet-async": "1.3.0",
    "react-hot-toast": "2.4.1",
    "react-router-dom": "6.20.1",
    "react-scripts": "5.0.1",
    "web-vitals": "5.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "6.0.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.0",
    "autoprefixer": "10.4.21",
    "eslint": "8.50.0",
    "eslint-config-react-app": "7.0.1",
    "html-validate": "8.7.0",
    "http-server": "14.1.1",
    "postcss": "8.5.6",
    "prettier": "3.0.0",
    "puppeteer": "21.5.2",
    "react-snap": "1.23.0",
    "tailwindcss": "3.4.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node build-fixed.js",
    "build:original": "react-scripts build",
    "build:netlify": "node build-netlify.js",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "validate:html": "node ../scripts/validate-html.js build",
    "validate:seo": "node ../scripts/validate-seo.js build",
    "serve": "npx http-server build -p 3000 -s",
    "deploy:cache-bust": "node cache-bust-deployment.js",
    "deploy:clean": "npm run deploy:cache-bust && npm run build",
    "validate:deployment": "node validate-linux.js",
    "build:validate": "npm run build && npm run validate:deployment"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "reactSnap": {
    "inlineCss": true,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "crawlFrom": "/",
    "include": [
      "/",
      "/naming-tool",
      "/features",
      "/pricing",
      "/how-it-works",
      "/examples",
      "/faq",
      "/contact",
      "/blog",
      "/privacy-policy",
      "/terms-of-service"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "waitFor": 3000,
    "fixWebpackChunksIssue": "CRA2",
    "removeBlobs": true,
    "fixInsertRule": true,
    "preconnectThirdParty": false,
    "removeStyleTags": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-web-security",
      "--disable-features=VizDisplayCompositor"
    ],
    "puppeteerExecutablePath": null,
    "puppeteerLaunchOptions": {
      "headless": true,
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--disable-web-security",
        "--disable-features=VizDisplayCompositor",
        "--disable-gpu",
        "--disable-dev-shm-usage",
        "--disable-extensions",
        "--no-zygote",
        "--single-process"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}